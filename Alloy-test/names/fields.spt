module names-fields

language Alloy

setup Common [[
	module test
]]

test Duplicate field error [[
	sig A {}
	sig B {[[a]]:A, [[a]]:A}
]] 2 errors

test Duplicate field error 2 [[
	sig A {}
	sig B {[[a]], [[a]]:A}
]] 2 errors

test Resolve ref to field [[
	sig A {}
	sig B {[[a]]:A}
	
	pred show (b:B){
		#b.[[a]]>1
	}
]] resolve #2 to #1

test Resolve ref to field in super sig [[
sig A {}
sig B {[[a]]:A}
sig C extends B{}

pred show (b:C){
	#b.[[a]]>1
}
]] resolve #2 to #1

